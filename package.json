{
  "name": "ai-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration development",
    "build:ci": "ng build --configuration ci",
    "build:ci-dev": "ng build --configuration ci-dev",
    "srv:compile": "npm --prefix functions run build",
    "fb:use": "firebase use",
    "fb:deploy": "firebase deploy",
    "srv:run": "firebase emulators:start --inspect-functions --export-on-exit em-data --import em-data",
    "srv:kill-ports": "npx kill-port 8080 5001 9099 9199 5000 4000 4500 9150",
    "srv:emulators": "npm run srv:kill-ports && npm run srv:run",
    "srv": "npm run srv:compile && npm run srv:run",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.17.2",
    "firebase-tools": "^14.12.1",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
